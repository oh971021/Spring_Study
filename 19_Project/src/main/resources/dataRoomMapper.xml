<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.js.pj.dataroom.DataRoomMapper">
	
	<insert id="upload" parameterType="com.js.pj.dataroom.DataRoomFile">
		insert into pj_dataroom	values(pj_dataroom_seq.nextval, #{d_owner}, #{d_title}, #{d_file}, #{d_category})
	</insert>

	<select id="get" resultType="com.js.pj.dataroom.DataRoomFile">
		select * from pj_dataroom order by d_title
	</select>

	<delete id="delete" parameterType="com.js.pj.dataroom.DataRoomFile">
		delete from pj_dataroom where d_no = #{d_no}
	</delete>
	
	
</mapper>