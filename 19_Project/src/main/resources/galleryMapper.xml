<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.js.pj.gallery.GalleryMapper">
	<select id="get" resultType="com.js.pj.gallery.GalleryFile">
		select * from pj_gallery order by g_no desc
	</select>
	
	<insert id="upload" parameterType="com.js.pj.gallery.GalleryFile">
	insert into pj_gallery
		values(pj_gallery_seq.nextval, #{g_owner}, #{g_title},
		#{g_file})
	</insert>
	
	<delete id="delete"
		parameterType="com.js.pj.gallery.GalleryFile">
		delete from mycafe_gallery where g_no = #{g_no}
	</delete>
</mapper>